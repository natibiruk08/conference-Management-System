viewApprovedWorkShops.js","original":{"line":31,"column":20}},{"generated":{"line":121,"column":9},"source":"workShop/viewApprovedWorkShops.js","original":{"line":28,"column":16}},{"generated":{"line":121,"column":10},"source":"workShop/viewApprovedWorkShops.js","original":{"line":26,"column":12}},{"generated":{"line":121,"column":11},"source":"workShop/viewApprovedWorkShops.js","original":{"line":25,"column":8}},{"generated":{"line":122,"column":0},"source":"workShop/viewApprovedWorkShops.js","original":{"line":66,"column":5}},{"name":"Component","generated":{"line":126,"column":2},"source":"workShop/viewApprovedWorkShops.js","original":{"line":7,"column":36}},{"name":"ViewApprovedWorkShops","generated":{"line":128,"column":15},"source":"workShop/viewApprovedWorkShops.js","original":{"line":69,"column":15}}],"sources":{"workShop/viewApprovedWorkShops.js":"import React, { Component } from 'react';\nimport axios from \"axios\";\nimport moment from \"moment\";\nimport \"./style/viewApprovedWorkShop.css\"\nimport upcoming from './images/upcoming.svg';\n\nclass ViewApprovedWorkShops extends Component {\n    constructor(props) {\n        super(props);\n        this.state = {\n            workshops: [],\n            isExpandClick: false\n        }\n    }\n\n    //to call the end point and get the values using axios\n    componentDidMount() {\n        axios.get('http://localhost:8080/workshop/')\n            .then(response => {\n                this.setState({ workshops: response.data.data })\n            })\n    }\n\n    render() {\n        return (\n            <div className=\"background-workshop\">\n                <img src={upcoming} />\n                <div className=\"container p-3\">\n                    <h1>UPCOMING CONFERENCES</h1><br />\n                    {/* Check whether array have any value */}\n                    <div className=\"p-3\">\n                        {this.state.workshops.length > 0 && this.state.workshops.map((item, index) => (\n                            <div key={index} >\n                                {item.status == \"approved\" &&\n                                    <div className=\"card border-warning shadow mb-5 bg-body rounded\">\n                                        <div className=\"card-header\">\n                                            <h5>Conference Title: {item.workShopTitle}</h5>\n                                        </div>\n                                        <div className=\"card-body\">\n                                            <div className=\"row\">\n                                                <dt className=\"col-sm-2\">Description</dt>\n                                                <dd className=\"col-sm-10\">{item.description}</dd>\n                                            </div>\n                                            <div className=\"row\">\n                                                <dt className=\"col-sm-2\">Presenters </dt>\n                                                <dd className=\"col-sm-10\">{item.presenters}</dd>\n                                            </div>\n                                            <div className=\"row\">\n                                                <dt className=\"col-sm-2\">Date</dt>\n                                                <dd className=\"col-sm-10\">{moment(item.date).format('YYYY-MM-DD')}</dd>\n                                            </div>\n                                            <div className=\"row\">\n                                                <dt className=\"col-sm-2\">Estimated Duration</dt>\n                                                <dd className=\"col-sm-10\">{item.estimatedDuration} days</dd>\n                                            </div>\n\n                                        </div>\n                                    </div>\n                                }\n                            </div>\n                        ))}\n                    </div>\n                </div>\n            </div>\n        )\n    }\n}\n\nexport default ViewApprovedWorkShops;"},"lineCount":null}},"error":null,"hash":"34c56c4e173d260d9bdea1cf3ce48899","cacheData":{"env":{}}}